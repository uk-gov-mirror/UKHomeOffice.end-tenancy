{
  "name": "end-tenancy",
  "version": "0.1.0",
  "main": "index.js",
  "description": "End Tenancy form",
  "author": "HomeOffice",
  "bugs": "https://github.com/UKHomeOffice/end-tenancy/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/UKHomeOffice/end-tenancy"
  },
  "engines": {
    "node": "^14.15.0"
  },
  "license": "./LICENSE",
  "readme": "./README.md",
  "scripts": {
    "start": "node server.js",
    "start:dev": "NODE_ENV=development hof-build watch",
    "ci": "NODE_ENV=ci DISABLE_CSP=true CASEWORKER_EMAIL=test@example.com node .",
    "test": "npm run test:lint && npm run test:unit",
    "test:unit": "nyc _mocha \"test/_unit/**/*.spec.js\"",
    "test:acceptance": "so-acceptance --steps",
    "test:acceptance:local": "funkie --driver chromedriver npm run test:acceptance",
    "test:lint": "npm run test:lint:app",
    "test:lint:app": "eslint . --config ./node_modules/eslint-config-hof/default.js",
    "test:lint:acceptance": "eslint --no-ignore -c ./node_modules/so-acceptance/.eslintrc ./apps/*/acceptance/**/*.js",
    "test:snyk": "snyk config set api=SNYK_TOKEN && snyk test",
    "build": "hof-build",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "debug": "^4.3.2",
    "hof": "^19.11.23",
    "hogan.js": "^3.0.2",
    "is-pdf": "^1.0.0",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "mixwith": "^0.1.1",
    "moment": "^2.29.1",
    "typeahead-aria": "^1.0.4"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.32.0",
    "eslint-config-hof": "^1.2.1",
    "express": "^4.17.1",
    "funkie": "^0.0.6",
    "funkie-chromedriver": "^0.1.0",
    "mocha": "^9.0.3",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "snyk": "^1.679.0",
    "so-acceptance": "^6.0.1"
  },
  "resolutions": {
    "underscore": "^1.12.1"
  },
  "mocha": {
    "reporter": "spec",
    "require": "test/setup.js",
    "recursive": "true",
    "timeout": "6000"
  }
}
